{
  "name": "@holepunchto/react-native-paste-input",
  "version": "0.8.1",
  "description": "React Native TextInput replacement to allow pasting files",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-paste-input.podspec",
    "!lib/typescript/example",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!example",
    "!.vscode",
    "!**/.idea",
    "!**/.gradle",
    "!android/build",
    "!ios/Build",
    ".circleci"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build && husky install",
    "release": "release-it",
    "example": "cd example && npm",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "npm run example i && npm run pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "paste",
    "text-input"
  ],
  "repository": "https://github.com/holepunchto/react-native-paste-input",
  "author": "Elias Nahum <nahumhbl@gmail.com> (https://github.com/mattermost)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/holepunchto/react-native-paste-input/issues"
  },
  "homepage": "https://github.com/holepunchto/react-native-paste-input#readme",
  "dependencies": {
    "semver": "7.6.3"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "19.6.0",
    "@react-native/eslint-config": "0.76.5",
    "@react-native/metro-config": "0.76.5",
    "@react-native/typescript-config": "0.76.5",
    "@release-it/conventional-changelog": "9.0.3",
    "@types/react": "18.3.5",
    "@typescript-eslint/eslint-plugin": "7.14.1",
    "@typescript-eslint/parser": "7.14.1",
    "commitlint": "19.6.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-ft-flow": "3.0.11",
    "eslint-plugin-jest": "28.9.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-native": "4.1.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "pod-install": "0.3.2",
    "prettier": "3.4.2",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-native-builder-bob": "0.35.2",
    "release-it": "17.10.0",
    "typescript": "5.7.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": false
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "conventionalcommits"
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "codegenConfig": {
    "name": "PasteTextInputSpecs",
    "type": "components",
    "jsSrcsDir": "./src",
    "includesGeneratedCode": false,
    "outputDir": {
      "ios": "./ios"
    },
    "android": {
      "javaPackageName": "com.mattermost.pastetextinput"
    }
  }
}
